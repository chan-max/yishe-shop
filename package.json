{
    "name": "1s",
    "version": "0.0.0",
    "description": "The most creative clothing design community",
    "author": "jackie c",
    "private": false,
    "license": "MIT",
    "type": "module",
    "main": "./nuxt.config.ts",
    "repository": {
        "type": "git",
        "url": ""
    },
    "files": [
        "assets",
        "components",
        "composables",
        "layouts",
        "modules",
        "pages",
        "public",
        "stores",
        "app.config.ts",
        "app.vue",
        "nuxt.config.ts",
        "tailwind.config.ts",
        "tsconfig.json"
    ],
    "scripts": {
        "build": "nuxt build ./app",
        "dev": "nuxt dev --port 15201 --host",
        "generate": "nuxt generate ./app",
        "preview": "nuxt preview ./app",
        "postinstall": "nuxt prepare ./app",
        "dev:starter": "nuxt dev ./examples/starter",
        "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
        "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
        "release": "release-it",
        "build:prod": "NODE_ENV=production nuxt generate",
        "build:dev": "NODE_ENV=development nuxt generate",
        "build:locales": "node scripts/build-locales.js",
        "build:en": "NUXT_PUBLIC_LOCALE=en nuxt generate",
        "build:zh": "NUXT_PUBLIC_LOCALE=zh nuxt generate"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "@nuxt/content": "^2.9.0",
        "@nuxt/devtools": "^1.0.0",
        "@nuxtjs/color-mode": "^3.3.0",
        "@nuxtjs/sitemap": "^3.1.0",
        "@nuxtjs/tailwindcss": "^6.8.1",
        "@pinia/nuxt": "^0.5.1",
        "@primeuix/themes": "^1.1.1",
        "@vueuse/core": "^10.5.0",
        "@vueuse/nuxt": "^10.7.0",
        "@yeger/vue-masonry-wall": "^5.1.0",
        "nuxt-headlessui": "^1.1.4",
        "nuxt-icon": "^0.6.1",
        "pinia": "^2.1.7",
        "sass": "^1.69.5",
        "stringify": "^5.2.0",
        "tailwindcss": "^3.3.5",
        "ua-parser-js": "^2.0.3",
        "unplugin-vue-components": "^0.25.2",
        "vuetify": "^3.8.9"
    },
    "devDependencies": {
        "@nuxtjs/eslint-config-typescript": "^12.1.0",
        "@nuxtjs/eslint-module": "^4.1.0",
        "@nuxtjs/i18n": "^10.1.0",
        "@nuxtjs/seo": "^3.2.2",
        "consola": "^3.2.3",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "nuxt": "^3.8.0",
        "prettier": "^3.0.3",
        "release-it": "^16.2.1",
        "serve": "^14.2.1",
        "typescript": "^5.2.2",
        "vue-tsc": "1.8.22"
    },
    "release-it": {
        "github": {
            "release": true,
            "releaseName": "v${version}"
        },
        "git": {
            "commitMessage": "chore(release): release v${version}"
        }
    }
}
